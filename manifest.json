{
    "name": "Pommy",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Timer plus a task list",
    "author":"Bruno Ribeiro",
    "action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "icons": {
        "128": "images/icon.png"
      },
      "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "http://*.//"
      ]
    }
  ]
}